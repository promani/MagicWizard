{
    "name": "promani/magicwizard-bundle",
    "type": "symfony-bundle",
    "description": "Wizard generator for Symfony applications",
    "keywords": ["symfony","wizard", "steper", "vue"],
    "homepage": "https://github.com/promani/MagicWizard",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignacio Pehu√©n Romani"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/twig-bundle": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "psr/log": "~1.0",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/test-pack": "^1.0",
        "symplify/easy-coding-standard": "^8.2",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "MagicWizardBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/Fixtures/TestApp/src/",
            "MagicWizardBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
